[run]
source = shop_api
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    shop_api/chat_*.py
    shop_api/models.py
    shop_api/db_models.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

